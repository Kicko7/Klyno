{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
      "builder": "NIXPACKS",
      "buildCommand": "pnpm install --no-frozen-lockfile"
    },
    "deploy": {
      "startCommand": "pnpm run dev:server",
      "healthcheckPath": "/",
      "restartPolicyType": "ON_FAILURE"
    }
  }
  